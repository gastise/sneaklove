<td class="table-division">
  <div><img src="{{this.picture}}" alt="img" class="table-image" /></div>
</td>
<td class="table-division">
  <div>{{this.name}}</div>
</td>
<td class="table-division">
  <div>{{this.ref}}</div>
</td>
<td class="table-division">
  <div>${{this.price}}</div>
</td>
<td class="table-division">
  <a
    href="/product-edit/{{this._id}}"
    data-id-sneaker="{{this._id}}"
    {{!-- class="fa fa-edit table-icon" --}}
  >Edit</a>
</td>
<td class="table-division">
  <a
    href="/product-delete/{{this.id}}"
    {{!-- class="fa fa-trash table-icon" --}}
    data-id-sneaker="{{this._id}}"
  >Delete</a>
</td>